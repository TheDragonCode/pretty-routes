{
    "name": "dragon-code/pretty-routes",
    "description": "Pretty Routes for Laravel",
    "keywords": ["laravel", "routes", "pretty", "pretty-routes", "dragon-code", "dragon", "andrey-helldar"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        },
        {
            "name": "Gary Green",
            "email": "holegary@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/TheDragonCode/pretty-routes/issues",
        "source": "https://github.com/TheDragonCode/pretty-routes"
    },
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "dragon-code/contracts": "^2.4",
        "dragon-code/laravel-routes-core": "^4.0",
        "dragon-code/support": "^5.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "illuminate/http": "^6.0|^7.0|^8.0",
        "illuminate/routing": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "illuminate/view": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "PrettyRoutes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "PrettyRoutes\\ServiceProvider"
            ]
        },
        "thanks": [
            {
                "name": "dragon-code/laravel-routes-core",
                "url": "https://github.com/TheDragonCode/laravel-routes-core"
            },
            {
                "name": "dragon-code/support",
                "url": "https://github.com/TheDragonCode/support"
            }
        ]
    }
}
